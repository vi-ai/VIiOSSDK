# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

source 'https://github.com/CocoaPods/Specs.git'

workspace 'Samples.xcworkspace'

target 'VIAdMobSample' do
    use_frameworks!
    project 'VIAdMobSample/VIAdMobSample.xcodeproj'
    pod 'Google-Mobile-Ads-SDK'
    pod 'VISDK'
end

target 'VIAdMobSampleTests' do
    use_frameworks!
    project 'VIAdMobSample/VIAdMobSample.xcodeproj'
    
    pod 'Quick'
    pod 'Nimble'
    pod 'VISDK'
end


###
target 'VIMopubSample' do
    use_frameworks!
    project 'VIMopubSample/VIMopubSample.xcodeproj'
    pod 'mopub-ios-sdk'
    pod 'VISDK'
end

target 'VIMopubSampleTests' do
    use_frameworks!
    project 'VIMopubSample/VIMopubSample.xcodeproj'
    
    pod 'Quick'
    pod 'Nimble'
    pod 'VISDK'
end

####

target 'VIFBSample' do
    use_frameworks!
    project 'VIFBSample/VIFBSample.xcodeproj'
    pod 'FBAudienceNetwork', '~> 4.25.0'
    pod 'VISDK'
end

target 'VIFBSampleTests' do
    use_frameworks!
    project 'VIFBSample/VIFBSample.xcodeproj'

    pod 'Quick'
    pod 'Nimble'
    pod 'VISDK'
end

target 'VIADSample' do
	use_frameworks!
	project 'VIADSample/VIADSample.xcodeproj'
	pod 'VISDK'
end


